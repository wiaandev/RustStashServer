{
  "incremental": true,
  "lineWidth": 80,
  "indentWidth": 2,
  "typescript": {
    "locked": true,
    "spaceSurroundingProperties": false,
    "importDeclaration.spaceSurroundingNamedImports": false,
    "typeLiteral.spaceSurroundingProperties": false,
    "exportDeclaration.spaceSurroundingNamedExports": false,
    "quoteStyle": "preferSingle",
    "module.sortImportDeclarations": "maintain"
  },
  "json": {
    "locked": true
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,cs}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/obj/**",
    "**/bin/**",
    "**/dist/**",
    "**/schema.json",
    "**/__generated__/**",
    "**/Migrations/*.cs",
    "**/Data/**",
    "**/*.generated.cs"
  ],
  "roslyn": {
    "locked": true,
    "indentWidth": 4
  },
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.68.2.wasm",
    "https://plugins.dprint.dev/json-0.15.2.wasm",
    "https://plugins.dprint.dev/roslyn-0.6.1.exe-plugin@e8dbd4794e1e89400c4f2311508a863d28161b626dd71c594c4147f547d47fa1"
  ]
}
